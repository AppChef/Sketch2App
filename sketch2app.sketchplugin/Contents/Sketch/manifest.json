{
  "name": "Sketch2App",
  "description": "A Sketch Plugin to upload mobile design and create native app instantly. Design in Sketch and view result on device in real time.",
  "version": 0.1,
  "identifier": "instantapp.io.sketch2app",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Upload Sketch File",
      "identifier": "upload",
      "shortcut": "ctrl alt shift 1",
      "script": "upload.sketchscript"
    },
    {
      "name": "Install SketchTool",
      "identifier": "install-sketchtool",
      "shortcut": "ctrl alt shift 2",
      "script": "installSketchTool.sketchscript"
    },
    {
      "name": "Log out",
      "identifier": "logout",
      "shortcut": "ctrl alt shift 3",
      "script": "logout.sketchscript"
    }
  ],
  "menu": {
    "title": "ðŸ“² Sketch2App",
    "items": [
      "upload",
      "install-sketchtool",
      "logout"
    ]
  }
}
