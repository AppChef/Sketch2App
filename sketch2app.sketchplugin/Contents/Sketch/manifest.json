{
  "name": "AppChef Sketch2App",
  "description":
    "A Sketch Plugin to upload mobile design and create native app instantly. Design in Sketch and view result on device in real time.",
  "version": "0.4.5",
  "identifier": "appchef.io.sketch2app",
  "appcast":
    "https://api.sketchpacks.com/v1/plugins/appchef.io.sketch2app/appcast",
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "commands": [
    {
      "name": "Upload Current File to AppChef",
      "identifier": "upload",
      "shortcut": "cmd shift 1",
      "script": "upload.cocoascript"
    },
    {
      "name": "Install SketchTool (Required after Sketch update)",
      "identifier": "install-sketchtool",
      "script": "installSketchTool.cocoascript"
    },
    {
      "name": "Log out",
      "identifier": "logout",
      "script": "logout.cocoascript"
    }
  ],
  "menu": {
    "title": "ðŸ“² AppChef Sketch2App",
    "items": ["upload", "install-sketchtool", "-", "logout"]
  }
}
